package auxiliar;

public class Aluno {
	private String nome;
	private int idade;
	private String  matricula;
	
	
	public void setNome(String nome) {
		this.nome=nome;
	}
	public void setIdade(int idade) {
		this.idade=idade;
	}
	public void setMatricula(String matricula) {
		this.matricula=matricula;
	}

}
